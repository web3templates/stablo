{
    "node": true,
    "browser": true,
    "esnext": true,
    "bitwise": false,
    "curly": true,
    "eqeqeq": true,
    "immed": true,
    "indent": 4,
    "latedef": "nofunc",
    "newcap": true,
    "noarg": true,
    "quotmark": "single",
    "regexp": true,
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true,
    "smarttabs": true,
    "laxcomma": true,

    "predef": [
        "require",
        "define"
    ]
}
